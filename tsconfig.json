{
  "compilerOptions": {
    "outDir": "./build/lib/esm",
    "module": "es2015",
    "target": "es5",
    "lib": ["es2017", "dom"],
    "jsx": "react",
    "moduleResolution": "node",
    "baseUrl": ".",
    "declaration": true,
    "strict": true,
    "noImplicitAny": true,
    "types": ["svelte"],
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/**/*.ts", "scripts/templates.ts", "ai/AiFillA.tls", "ai/AiFillAlert.js"],
  "exclude": ["icons"]
}
